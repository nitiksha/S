package pack

object Tuple {

  val mytuple = (1,3,"he",true);
  val mytuple1 = new Tuple4(1,3,"the",false);
  val mytuple2 = new Tuple3(1,3,(2,3));
  def main(args: Array[String]): Unit = { println(mytuple);
    println(mytuple1);println(mytuple1._2);

    mytuple.productIterator.foreach{ i => println(i)
    }
    println(mytuple2)
    println(mytuple2._3)
    println(mytuple2._3._2)
  }

}
